<!-- application_library -->
<!ELEMENT application_library (application*)>
<!ATTLIST application_library name NMTOKEN #REQUIRED>

<!-- application -->
<!ELEMENT application (domain_participant)+>
<!ATTLIST application name CDATA #REQUIRED>

<!-- domain_participant -->
<!ELEMENT domain_participant (
                        memory_management?,
                        (register_type* | 
                        topic*), 
                        (publisher* | 
                        subscriber* | 
                        data_writer* | 
                        data_reader* | 
                        participant_qos?))>
